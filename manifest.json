{
  "manifest_version": 2,

  "name": "Ship Center",
  "description": "Add your SKU and quantity information your Amazon shipment label",
  "version": "1.1",

  "browser_action": {
    "default_icon" : "assets/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://sellercentral.amazon.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://unpkg.com/pdf-lib; object-src 'self'"
}
