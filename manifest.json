{
  "manifest_version": 2,

  "name": "Extension Name",
  "description": "Some Desc.",
  "version": "1.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "storage",
    "*://*/*",
    "ftp://*/*"
  ],
  "background": {
    "scripts" : [
      "background/main.js"
    ]
  },

  "web_accessible_resources": ["injected.js"]
}
